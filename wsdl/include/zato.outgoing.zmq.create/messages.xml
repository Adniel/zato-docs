      <xsd:element name="zato_outgoing_zmq_create_request">
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="cluster_id" type="xsd:int"/>
            <xsd:element name="name" type="xsd:string"/>
            <xsd:element name="is_active" type="xsd:boolean"/>
            <xsd:element name="address" type="xsd:string"/>
            <xsd:element name="socket_type" type="xsd:string"/>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>

      <xsd:element name="zato_outgoing_zmq_create_response">
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element ref="zato_env"/>
            <xsd:element name="item" minOccurs="0" maxOccurs="1">
              <xsd:complexType>
                <xsd:sequence>
                  <xsd:element name="id" type="xsd:int"/>
                  <xsd:element name="name" type="xsd:string"/>
                </xsd:sequence>
              </xsd:complexType>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element> 